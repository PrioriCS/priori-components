{
  "name": "prioricomponents",
  "version": "0.11.2",
  "private": false,
  "main": "dist/index.js",
  "module": "dist/index.js",
  "style": "dist/output.css",
  "files": [
    "dist",
    "README.md"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PrioriCS/priori-components.git"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "@headlessui/react": "^1.7.17",
    "currency.js": "^2.0.4",
    "immutability-helper": "^3.1.1",
    "input-mask": "^1.1.20",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "query-string": "^7.1.1",
    "randomstring": "^1.2.2",
    "react-icons": "^4.10.1",
    "react-input-mask": "^2.0.4",
    "tailwind-merge": "^1.14.0",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "rm -rf dist && NODE_ENV=production babel src/lib --out-dir dist --copy-files && npx tailwindcss -i ./src/css/input.css -o ./dist/output.css",
    "test": "jest",
    "tailwind": "npx tailwindcss -i ./src/css/input.css -o ./dist/output.css --watch",
    "test:watch": "jest --watchAll",
    "test:coverage": "jest --coverage",
    "test:debug": "ndb jest",
    "eject": "react-scripts eject",
    "prepare": "husky install",
    "lint-staged": "lint-staged",
    "lint": "eslint ./src/**/*.{js,jsx,tsx} --fix",
    "version:patch": "npm version patch",
    "version:minor": "npm version minor",
    "version:major": "npm version major"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.22.9",
    "@babel/core": "^7.22.9",
    "@babel/preset-env": "^7.22.9",
    "@babel/preset-react": "^7.22.5",
    "@commitlint/cli": "^17.7.0",
    "@commitlint/config-conventional": "^17.7.0",
    "@commitlint/format": "^17.4.4",
    "@inertiajs/inertia-react": "^0.8.1",
    "@tailwindcss/forms": "^0.5.6",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^12.1.5",
    "@testing-library/user-event": "^14.4.3",
    "autoprefixer": "^10.4.15",
    "babel-jest": "^29.6.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.46.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-jest": "^27.2.3",
    "eslint-plugin-react": "^7.33.1",
    "eslint-plugin-tailwindcss": "^3.13.0",
    "husky": "^8.0.3",
    "jest": "^29.6.2",
    "jest-environment-jsdom": "^29.6.2",
    "jsconfig-paths-jest-mapper": "^1.0.0",
    "lint-staged": "^13.2.3",
    "prettier": "^3.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "5.0.1",
    "react-test-renderer": "^17.0.2",
    "tailwindcss": "^3.3.3"
  }
}
